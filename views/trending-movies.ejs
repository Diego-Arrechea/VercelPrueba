                <div class="browse-content">
                    <div class="container">
                        <h2><b>24h Trending Movies</b></h2>
                        <section>
                            <div class="row">
                                <% movies.forEach(movie=> { %>
                                    <div class="browse-movie-wrap col-xs-10 col-sm-4 col-md-5 col-lg-4">
                                        <a href="https://yts.mx/movies/<%= movie.url %>" class="browse-movie-link">
                                            <figure>
                                                <img class="img-responsive" src="<%= movie.portada %>"
                                                    alt="<%= movie.subtitle %> (<%= movie.year %>) descargar"
                                                    width="170" height="255">
                                                <figcaption class="hidden-xs hidden-sm">
                                                    <span class="icon-star"></span>
                                                    <h4 class="rating">
                                                        <%= movie.rating %> / 10
                                                    </h4>
                                                    <% movie.genre.forEach(genreItem=> { %>
                                                        <h4>
                                                            <%= genreItem %>
                                                        </h4>
                                                        <% }); %>
                                                            <span class="button-green-download2-big">View Details</span>
                                                </figcaption>
                                            </figure>
                                        </a>
                                        <div class="browse-movie-bottom">
                                            <a href="https://yts.mx/movies/<%= movie.url %>" class="browse-movie-title">
                                                <% if (movie.country !==null) { %>
                                                    <span style="color: #ACD7DE; font-size: 75%;">[PL]</span>
                                                    <% } %>
                                                        <%= movie.subtitle %>
                                            </a>
                                            <div class="browse-movie-year">
                                                <%= movie.year %>
                                            </div>
                                        </div>
                                    </div>
                                    <% }); %>
                            </div>
                        </section>
                    </div>
